{
	"name": "digital_hippo",
	"version": "0.1.0",
	"private": true,
	"main": "dist/app/server.js",
	"scripts": {
		"dev": "cross-env PAYLOAD_CONFIG_PATH=app/payload.config.ts nodemon",
		"generate:types": "cross-env PAYLOAD_CONFIG_PATH=app/payload.config.ts payload generate:types",
		"build:payload": "cross-env PAYLOAD_CONFIG_PATH=app/payload.config.ts payload build",
		"build:server": "tsc --project tsconfig.server.json",
		"build:next": "next build",
		"build": "npm run build:payload && npm run build:server && npm run build:next",
		"start": "cross-env PAYLOAD_CONFIG_PATH=dist/app/payload.config.js NODE_ENV=production node dist/app/server.js",
		"copyfiles": "copyfiles -u 1 \"app/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"lint": "next lint"
	},
